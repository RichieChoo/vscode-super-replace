{
    "name": "super-replace",
    "displayName": "Super Replace",
    "description": "Advanced regexp replace which allows you assign additional text processing function when replacing.",
    "version": "0.0.1",
    "publisher": "jebbs",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:superReplace.replaceWindow"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "superReplace.replaceWindow",
                "title": "Open Replace Window...",
                "category": "Super Replace"
            }
        ],
        "configuration":[
            {
                "title": "Super Replace Configuration",
                "properties": {
                    "superReplace.googleApiKey":{
                        "type":"string",
                        "default":"",
                        "description": "Type your google API key in order to make google translator to work",
                        "scope": "window"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "build": "node ./build.js"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "google-translate": "^2.1.0"
    }
}
